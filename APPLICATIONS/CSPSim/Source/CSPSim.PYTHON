#!/usr/bin/python

import sys

# enable lazy loading of shared library modules
sys.setdlopenflags(0x101)

import SimData
import CSP

config = "../Data/CSPSim.ini"

if not CSP.openConfig(config):
	print "Unable to open primary configuration file (%s)" % config
	sys.exit(0)
	
app = CSP.CSPSim()

app.Init()
app.Run()
app.Cleanup()
app.Exit()

print "CSPSim normal exit."
