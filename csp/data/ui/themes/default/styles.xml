<?xml version="1.0"?>
<StyleDocument xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../window_document.xsd">
	<NamedStyles>
		<!-- Each of the following styles is default for each widget. -->
		<Style Name="Button">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>000000ff</Color>
			<BackgroundColor>ffffffff</BackgroundColor>
			<BackgroundImage>images/button.png</BackgroundImage>
			<BorderWidth>0</BorderWidth>
			<VerticalAlign>none</VerticalAlign>
			<HorizontalAlign>none</HorizontalAlign>
		</Style>

		<Style Name="Button:hover">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>ffffffff</Color>
			<BackgroundColor>ddddddff</BackgroundColor>
			<BackgroundImage>images/button.png</BackgroundImage>
			<BorderWidth>0</BorderWidth>
			<VerticalAlign>none</VerticalAlign>
			<HorizontalAlign>none</HorizontalAlign>
		</Style>

		<Style Name="Button:disabled">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>666666ff</Color>
			<BackgroundColor>ffffff22</BackgroundColor>
			<BackgroundImage>images/button.png</BackgroundImage>
			<BorderWidth>0</BorderWidth>
			<VerticalAlign>none</VerticalAlign>
			<HorizontalAlign>none</HorizontalAlign>
		</Style>

		<Style Name="Button:disabled:hover">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>666666ff</Color>
			<BackgroundColor>ffffff22</BackgroundColor>
			<BackgroundImage>images/button.png</BackgroundImage>
			<BorderWidth>0</BorderWidth>
			<VerticalAlign>none</VerticalAlign>
			<HorizontalAlign>none</HorizontalAlign>
		</Style>
		
		<Style Name="Check">
			<BackgroundColor>ffffffff</BackgroundColor>
			<BackgroundImage>images/unchecked.png</BackgroundImage>
		</Style>

		<Style Name="Check:checked">
			<BackgroundColor>ffffffff</BackgroundColor>
			<BackgroundImage>images/checked.png</BackgroundImage>
		</Style>

		<Style Name="Check:checked:hover">
			<BackgroundColor>ffffffff</BackgroundColor>
			<BackgroundImage>images/checked_hover.png</BackgroundImage>
		</Style>

		<Style Name="CheckBox">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>000000ff</Color>
			<BorderWidth>0</BorderWidth>
			<BorderColor>000000ff</BorderColor>
		</Style>
		
		<Style Name="Image">
			<Color>000000ff</Color>
			<BackgroundColor>00000000</BackgroundColor>
			<VerticalAlign>none</VerticalAlign>
			<HorizontalAlign>none</HorizontalAlign>
		</Style>

		<Style Name="Label">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>000000ff</Color>
			<TextHorizontalAlign>left</TextHorizontalAlign>
			<BackgroundColor>00000000</BackgroundColor>
			<VerticalAlign>none</VerticalAlign>
			<HorizontalAlign>none</HorizontalAlign>
		</Style>

		<Style Name="Label:disabled">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>666666ff</Color>
			<TextHorizontalAlign>left</TextHorizontalAlign>
			<BackgroundColor>00000000</BackgroundColor>
			<VerticalAlign>none</VerticalAlign>
			<HorizontalAlign>none</HorizontalAlign>
		</Style>

		<Style Name="ListBox">
			<BorderWidth>1</BorderWidth>
			<BorderColor>b0b0b0ff</BorderColor>
		</Style>

		<Style Name="ListBoxItem">
			<BackgroundColor>ffffff55</BackgroundColor>
			<BorderWidth>0</BorderWidth>
			<Height>30</Height>
		</Style>

		<Style Name="ListBoxItem:selected">
			<BackgroundColor>7b869855</BackgroundColor>
			<BackgroundColorBottomLeft>7b869855</BackgroundColorBottomLeft>
			<BackgroundColorBottomRight>7b869855</BackgroundColorBottomRight>
			<BorderWidth>0</BorderWidth>
		</Style>

		<Style Name="ListBoxItem:hover:selected">
			<BackgroundColor>7b869855</BackgroundColor>
			<BackgroundColorBottomLeft>7b869855</BackgroundColorBottomLeft>
			<BackgroundColorBottomRight>7b869855</BackgroundColorBottomRight>
			<BorderWidth>0</BorderWidth>
		</Style>

		<Style Name="Tab">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>20</FontSize>
			<Color>000000ff</Color>
		</Style>
		
		<Style Name="TabHeader">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>333333ff</Color>
			<BackgroundColor>f0f0f0ff</BackgroundColor>
			<BackgroundColorBottomLeft>80808088</BackgroundColorBottomLeft>
			<BackgroundColorBottomRight>80808088</BackgroundColorBottomRight>
			<BorderWidth>1</BorderWidth>
			<BorderBottomWidth>0</BorderBottomWidth>
			<BorderColor>80808088</BorderColor>
		</Style>

		<Style Name="TabHeader:hover">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>000000ff</Color>
			<BackgroundColor>f0f0f0ff</BackgroundColor>
			<BackgroundColorBottomLeft>f0f0f0ff</BackgroundColorBottomLeft>
			<BackgroundColorBottomRight>f0f0f0ff</BackgroundColorBottomRight>
			<BorderWidth>1</BorderWidth>
			<BorderBottomWidth>0</BorderBottomWidth>
			<BorderColor>7b8698ff</BorderColor>
		</Style>

		<Style Name="TabHeader:hover:selected">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>000000ff</Color>
			<BackgroundColor>f0f0f0ff</BackgroundColor>
			<BackgroundColorBottomLeft>f0f0f0ff</BackgroundColorBottomLeft>
			<BackgroundColorBottomRight>f0f0f0ff</BackgroundColorBottomRight>
			<BorderWidth>1</BorderWidth>
			<BorderBottomWidth>0</BorderBottomWidth>
			<BorderColor>7b8698ff</BorderColor>
		</Style>

		<Style Name="TabHeader:selected">
			<FontFamily>prima_sans_bt.ttf</FontFamily>
			<FontSize>15</FontSize>
			<Color>000000ff</Color>
			<BackgroundColor>f0f0f088</BackgroundColor>
			<BackgroundColorBottomLeft>f0f0f0ff</BackgroundColorBottomLeft>
			<BackgroundColorBottomRight>f0f0f0ff</BackgroundColorBottomRight>
			<BorderWidth>1</BorderWidth>
			<BorderBottomWidth>0</BorderBottomWidth>
			<BorderColor>7b8698ff</BorderColor>
		</Style>

		<Style Name="TabPage">
			<BackgroundColor>f0f0f088</BackgroundColor>
			<BorderWidth>1</BorderWidth>
			<BorderColor>7b8698ff</BorderColor>
		</Style>

		<Style Name="Window">
			<BackgroundColor>fdfdfdcc</BackgroundColor>
			<BorderWidth>1</BorderWidth>
			<BorderColor>b0b0b0ff</BorderColor>
		</Style>

		<!-- The following styles is specialized styles that can be applied to any
			control. -->
			
		<Style Name="multiline">
			<TextVerticalAlign>top</TextVerticalAlign>
			<TextHorizontalAlign>left</TextHorizontalAlign>
		</Style>

		<Style Name="close_button">
			<BackgroundImage>images/close_button.png</BackgroundImage>
			<VerticalAlign>top</VerticalAlign>
			<HorizontalAlign>right</HorizontalAlign>
			<Height>32</Height>
			<Width>32</Width>
		</Style>

		<Style Name="desktop">
			<BackgroundColor>ffffffff</BackgroundColor>
			<BorderWidth>0</BorderWidth>
		</Style>
		
		<Style Name="heading">
			<FontSize>30</FontSize>
		</Style>

		<Style Name="mainmenu">
			<BackgroundColor>fdfdfdff</BackgroundColor>
			<BorderBottomWidth>1</BorderBottomWidth>
			<BorderRightWidth>1</BorderRightWidth>
			<VerticalAlign>top</VerticalAlign>
			<HorizontalAlign>left</HorizontalAlign>
		</Style>

		<Style Name="stripe">
			<BackgroundColor>f0f0f0ff</BackgroundColor>
			<BackgroundColorTopLeft>7b8698ff</BackgroundColorTopLeft>
			<BackgroundColorBottomLeft>7b8698ff</BackgroundColorBottomLeft>
			<BorderTopWidth>1</BorderTopWidth>
			<BorderBottomWidth>1</BorderBottomWidth>
			<BorderTopColor>7b8698ff</BorderTopColor>
			<BorderBottomColor>7b8698ff</BorderBottomColor>
			<Left>0</Left>
			<Top>35</Top>
			<Height>10</Height>
			<Width>100%</Width>
		</Style>

		<Style Name="tabpage_stripe">
			<BackgroundColor>f0f0f0ff</BackgroundColor>
			<BackgroundColorTopLeft>7b8698ff</BackgroundColorTopLeft>
			<BackgroundColorBottomLeft>7b8698ff</BackgroundColorBottomLeft>
			<BorderTopWidth>1</BorderTopWidth>
			<BorderBottomWidth>1</BorderBottomWidth>
			<BorderTopColor>7b8698ff</BorderTopColor>
			<BorderBottomColor>7b8698ff</BorderBottomColor>
			<Left>0</Left>
			<Top>0</Top>
			<Height>10</Height>
			<Width>100%</Width>
		</Style>

		<Style Name="window_default">
			<BorderLeftWidth>10</BorderLeftWidth>
			<BorderLeftColor>7b8698ff</BorderLeftColor>
			<VerticalAlign>middle</VerticalAlign>
			<HorizontalAlign>center</HorizontalAlign>
		</Style>
		
		<!-- This style should be used on a standard message box. -->
		<Style Name="messagebox">
			<BorderLeftWidth>10</BorderLeftWidth>
			<BorderLeftColor>7b8698ff</BorderLeftColor>
			<VerticalAlign>middle</VerticalAlign>
			<HorizontalAlign>center</HorizontalAlign>
			<Height>200</Height>
			<Width>500</Width>
		</Style>
		
		<!-- Use to place the message within a message box. -->
		<Style Name="messagebox_message">
			<Left>10</Left>
			<Top>55</Top>
			<Height>30</Height>
			<Width>489</Width>
		</Style>

		<!-- This style is used on the label that represents the caption
			 of a window. -->
		<Style Name="window_caption">
			<Left>10</Left>
			<Top>2</Top>
			<Height>30</Height>
			<Width>98%</Width>
		</Style>
	</NamedStyles>
</StyleDocument>
